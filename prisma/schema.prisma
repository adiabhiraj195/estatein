generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model estate_estate_and_location {
  id                 Int    @id @default(autoincrement())
  estate_name        String @unique
  // city_id     Int    @default(0)
  estate_type        String
  number_of_bedroom  Int
  number_of_bathroom Int
  estate_description String
  // estate_image BLOBs
}